{
  "name": "NauticApp",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "commitmsg": "commitlint -e",
    "release": "standard-version",
    "device": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android"
  },
  "dependencies": {
    "expo": "^28.0.0",
    "firebase": "*",
    "react": "16.2.0",
    "react-native": "*",
    "react-native-carousel-view": "*",
    "react-native-datepicker": "*",
    "react-native-fbsdk": "*",
    "react-native-fetch-blob": "*",
    "react-native-file-download": "*",
    "react-native-geocoder": "*",
    "react-native-google-signin": "^0.12.0",
    "react-native-maps": "*",
    "react-native-modal": "*",
    "react-native-modal-overlay": "*",
    "react-native-textinput-effects": "^0.4.2",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "*",
    "@commitlint/config-conventional": "*",
    "@commitlint/prompt": "*",
    "babel-jest": "*",
    "babel-preset-react-native": "*",
    "commitizen": "*",
    "husky": "*",
    "jest": "*",
    "react-test-renderer": "*",
    "standard-version": "*"
  },
  "jest": {
    "preset": "react-native"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  }
}
